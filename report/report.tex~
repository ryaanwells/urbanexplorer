\usepackage{url}
\usepackage{color}
\usepackage{graphics,graphicx}
\usepackage{epsfig}
\usepackage{epstopdf}
\usepackage{colortbl}
\usepackage{multirow}
\usepackage{booktabs}
\usepackage{ifthen}

\begin{document}

\newcommand{\todo}[1]{\textcolor{red}{#1}}
	
	
	\title{An Example Project}
\author{Ryan Wells}
\date{\today}
\maketitle
\begin{abstract}

The abstract goes here

\end{abstract}
